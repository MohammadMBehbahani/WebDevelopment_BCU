@{
    ViewData["Title"] = "UserProfile Page";
}
@model WebDevelopment_BCU.Models.ViewData.HomeData

<section class="news-detail section-padding pt-0">
    <div class="container">
        <div class="row">

            <div class="col-lg-12 col-12">

                <div class="col-lg-10 col-10 mx-auto mt-5">

                    <table class="table table-striped table-bordered table-hover">
                        <thead>
                            <tr>
                                <th>UserName</th>
                                <th>FullName</th>
                                <th>Orders</th>


                            </tr>
                        </thead>
                        <tbody>
                            <tr class="gradeA">
                                <td>@Model.User.UserName</td>
                                <td>@Model.User.FullName</td>

                                <td>
                                    <a href="/UserOrder/Index">Orders</a>
                                </td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>

        </div>
    </div>
</section>